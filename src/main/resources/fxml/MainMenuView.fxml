<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Kingdom.cards.Controllers.MainMenuController">
   <children>
      <GridPane layoutY="4.0" prefHeight="359.0" prefWidth="409.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnStart" mnemonicParsing="false" onAction="#btnStartCall" text="Start a new game" GridPane.rowIndex="0" />
             <Button fx:id="btnOptions" mnemonicParsing="false" onAction="#btnOptionsCall" text="Options" GridPane.rowIndex="1" />
             <Button fx:id="btnScore" mnemonicParsing="false" onAction="#btnScoreCall" text="HighScore" GridPane.rowIndex="2" />
             <Button fx:id="btnQuit" mnemonicParsing="false" onAction="#btnQuitCall" text="Quit the game" GridPane.rowIndex="3" />
            <Label fx:id="text" prefHeight="17.0" prefWidth="85.0" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
